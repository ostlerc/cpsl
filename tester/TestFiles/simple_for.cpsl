VAR a : integer;
    b : integer;

BEGIN

for a := 2 to 4 do
  write("a is ",a,'\n');
end;

write('\n');

for a := 4 downto 2 do
  write("a is ",a,'\n');
end;

write('\n');

b := 3;
for a := 2 to b-1 do
write("a is ",a,'\n');
end;

write('\n');

b := 5;
for a := b-1 downto b-1 do
write("a is ",a,'\n');
end;

b := 5;
for a := b-1 to b-1 do
write("a is ",a,'\n');
end;


END.
